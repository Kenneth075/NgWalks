@model IEnumerable<NgWalks.UI.Models.Dto.RegionDto>
@{

}

<h1 class="mt-3">Region</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Code</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var reg in Model)
        {
            <tr>
                <td>@reg.Id</td>
                <td>@reg.Code</td>
                <td>@reg.Name</td>
            </tr>
        }
    </tbody>
</table>